          <kb:trigger  type="pv" ref="PostgresJavaConnector" />
       </kb:event>
      <kb:event id="JaWapDbWithMysql" 
        nexusRef="JaWapDbSoSt"
        contextPath="JaWapDbSoSt storeData"> 
