    <kb:alternatives>
      <kb:fulfills purposeID="relishDinner" variantID="VeggiMeal CarniMeal" />
      <kb:fulfills purposeID="addTaste" variantID="Hollandaise BratenSosse" />
      <kb:fulfills purposeID="addVitamins" variantID="Spargel Erbsen" />
      <kb:fulfills purposeID="addProteins" variantID="Braten Schinken" />
      <kb:fulfills purposeID="addCarbohydrats" variantID="Kartoffeln Nudeln" />
    </kb:alternatives>  

    <kb:constituents>
      <kb:constitutes variantID="VeggiMeal" purposeID="addTaste addVitamins addCarbohydrats" />
      <kb:constitutes variantID="CarniMeal" purposeID="addTaste addVitamins addProteins addCarbohydrats" />
    </kb:constituents> 


    <kb:events>

      <kb:event id="VeggiMealWithTasteX" variantID="VeggiMeal">
        <kb:label>Veggi Meal mit TasteX</kb:label>
        <kb:contextPath pathIDs="VeggiMeal addTaste" /> 
      </kb:event>
      <kb:event id="VeggiMealWithHollandaise" variantID="VeggiMeal">
        <kb:label>Veggi Meal mit Hollandaise</kb:label>
        <kb:contextPath pathIDs="VeggiMeal addTaste Hollandaise" /> 
      </kb:event>

      <kb:event id="VeggiMealWithVitaminX" variantID="VeggiMeal">
        <kb:label>Veggi Meal mit VitaminX</kb:label>      
        <kb:contextPath pathIDs="VeggiMeal addVitamins" /> 
       </kb:event>
      <kb:event id="VeggiMealWithSpargel" variantID="VeggiMeal">
        <kb:label>Veggi Meal mit Spargel</kb:label>      
        <kb:contextPath pathIDs="VeggiMeal addVitamins Spargel" /> 
       </kb:event>      
 
       <kb:event id="CarniMealWithTasteX" variantID="CarniMeal">
        <kb:label>Carni Meal mit TasteX</kb:label>
        <kb:contextPath pathIDs="CarniMeal addTaste" /> 
      </kb:event>
      <kb:event id="CarniMealWithHollandaise" variantID="CarniMeal">
        <kb:label>Carni Meal mit Hollandaise</kb:label>
        <kb:contextPath pathIDs="CarniMeal addTaste Hollandaise" /> 
      </kb:event>
 
      <kb:event id="CarniMealWithVitaminX" variantID="CarniMeal">
        <kb:label>Carni Meal mit VitaminX</kb:label>
        <kb:contextPath pathIDs="CarniMeal addVitamins" /> 
      </kb:event>       
      <kb:event id="CarniMealWithSpargel" variantID="CarniMeal">
        <kb:label>Carni Meal mit Spargel</kb:label>
        <kb:contextPath pathIDs="CarniMeal addVitamins Spargel" /> 
      </kb:event>

       <kb:event id="CarniMealWithProteinX" variantID="CarniMeal">
        <kb:label>Carni Meal mit ProteinX</kb:label>
        <kb:contextPath pathIDs="CarniMeal addProteins" /> 
      </kb:event>      
      <kb:event id="CarniMealWithSchinken" variantID="CarniMeal">
        <kb:label>Carni Meal mit Schinken</kb:label>
        <kb:contextPath pathIDs="CarniMeal addProteins Schinken" /> 
      </kb:event>

      <kb:event id="CarniMealWithCarboX" variantID="CarniMeal">
        <kb:label>Carni Meal mit KohlehydratX</kb:label>
        <kb:contextPath pathIDs="CarniMeal addCarbohydrats" /> 
      </kb:event>      
      <kb:event id="CarniMealWithKartoffeln" variantID="CarniMeal">
        <kb:label>Carni Meal mit Kartoffeln</kb:label>
        <kb:contextPath pathIDs="CarniMeal addCarbohydrats Kartoffeln" /> 
      </kb:event>

      <kb:event id="VeggiMealWithCarboX" variantID="VeggiMeal">
        <kb:label>VeggiMeal mit KohlehydratX</kb:label>
        <kb:contextPath pathIDs="VeggiMeal addCarbohydrats" /> 
      </kb:event>
      
      <kb:event id="VeggiMealWithKartoffeln" variantID="VeggiMeal">
        <kb:label>VeggiMeal mit Kartoffeln</kb:label>
        <kb:contextPath pathIDs="VeggiMeal addCarbohydrats Kartoffeln" /> 
      </kb:event>
    </kb:events> 


    <kb:rules>

      <kb:rule 
        id="IFVMwithHoolandaiseTHENVMwithSpargel"
        variantID="VeggiMeal"
        triggerEventID="VeggiMealWithHollandaise"        
        premiseEventID="VeggiMealWithVitaminX"
        conclusionEventID="VeggiMealWithSpargel" >
        <kb:label>IF VM mit Hoolandaise THEN VM mit Spargel</kb:label>
      </kb:rule>

      <kb:rule 
        id="IFVMwithSpargelTHENVMwithHoolandaise"
        variantID="VeggiMeal"
        triggerEventID="VeggiMealWithSpargel"
        premiseEventID="VeggiMealWithTasteX"
        conclusionEventID="VeggiMealWithHollandaise" >
        <kb:label>IF VM mit Spargel THEN VM mit Hollandaise</kb:label>
      </kb:rule>

      <kb:rule 
        id="IFCMwithHoolandaiseTHENCMwithSpargel"
        variantID="CarniMeal"
        triggerEventID="CarniMealWithHollandaise"
        premiseEventID="CarniMealWithVitaminX"
        conclusionEventID="CarniMealWithSpargel" >
        <kb:label>IF CM mit Hoolandaise THEN CM mit Spargel</kb:label>
      </kb:rule>

      <kb:rule 
        id="IFCMwithSpargelTHENCMwithHoolandaise"
        variantID="CarniMeal"
        triggerEventID="CarniMealWithSpargel"
        premiseEventID="CarniMealWithTasteX"
        conclusionEventID="CarniMealWithHollandaise" >
        <kb:label>IF CM mit Spargel THEN CM mit Hollandaise</kb:label>
      </kb:rule>

      <kb:rule 
        id="IFCMwithSpargelTHENCMwithSchinken"
        variantID="CarniMeal"
        triggerEventID="CarniMealWithSpargel"
        premiseEventID="CarniMealWithProteinX"
        conclusionEventID="CarniMealWithSchinken" >
        <kb:label>IF CM mit Spargel THEN CM mit Schinken</kb:label>
      </kb:rule>

      <kb:rule 
        id="IFCMwithSpargelTHENCMwithKartoffeln"
        variantID="CarniMeal"
        triggerEventID="CarniMealWithSpargel"
        premiseEventID="CarniMealWithCarboX"
        conclusionEventID="CarniMealWithKartoffeln" >
        <kb:label>IF CM mit Spargel THEN CM mit Kartoffeln</kb:label>
      </kb:rule>

      <kb:rule 
        id="IFVMwithSpargelTHENVMwithKartoffeln"
        variantID="VeggiMeal"
        triggerEventID="VeggiMealWithSpargel"
        premiseEventID="VeggiMealWithCarboX"
        conclusionEventID="VeggiMealWithKartoffeln" >
        <kb:label>IF VM mit Spargel THEN VM mit Kartoffeln</kb:label>
      </kb:rule>
    </kb:rules> 

  </kb:data>    
