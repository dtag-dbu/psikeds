        <kb:label>Spargel</kb:label>
      </kb:variant>

      <kb:variant id="Erbsen">
        <kb:label>Erbsen</kb:label>
      </kb:variant>

      <kb:variant id="Schinken">
        <kb:label>Schinken</kb:label>
      </kb:variant>

      <kb:variant id="Braten">
