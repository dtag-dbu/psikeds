<kb:knowledgebase xmlns="http://org.psikeds.knowledgebase"
  xmlns:kb="http://org.psikeds.knowledgebase"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://org.psikeds.knowledgebase kb.xsd">
  <kb:meta>
    <kb:created>2014-04-06</kb:created>
    <kb:lastmodified>2014-04-16</kb:lastmodified>
    <kb:version>0.1.1</kb:version>
    <kb:creator>Karsten Reincke</kb:creator>
    <kb:description>Re-implementation of the first w7/kbsr LAMP demo database purely based
     on the elements which constitute the structure of an entity: purposes
     and variants</kb:description>
    <kb:description>This version demonstrates the fix of the semantic leak by using NOT events</kb:description>
  </kb:meta>
  <kb:data>


      
    <kb:purposes>
      <kb:purpose id="deliverStaticWebPages" root="true">
        <kb:label>deliver: Static Web Pages</kb:label>
        <kb:description>
        [something to] deliver HTML pages to the requesting browser whereas these
          pages are readily prepared and stored on the file system.
        </kb:description>
      </kb:purpose>
      <kb:purpose id="deliverDynamicWebPages" root="true">
        <kb:label>deliver: Dynamic Web Pages</kb:label>
        <kb:description>
        [something to] deliver dynamically computed HTML pages to the requesting 
           browser and to compose theses pages in the moment when they are requested.
        </kb:description>
      </kb:purpose>
      <kb:purpose id="deliverPersistentDynamicWebPages" root="true">
        <kb:label>deliver: Persistent Dynamic Web Pages</kb:label>
        <kb:description>
        [something to] deliver dynamically computed HTML pages to the requesting 
           browser and to compose theses pages in the moment when they are requested
           and to computed them on the base of persistently stored database data.
        </kb:description>
      </kb:purpose>
      <kb:purpose id="deliverPersistentJavaApplicationsOutput" root="true">
        <kb:label>deliver: Output of Persistently Operating Java Applications</kb:label>
        <kb:description>
        [something to] deliver the output of Java applications to a requesting 
           client whereas these Java applications shall be able to use persistently 
           stored database data for dynamically computing its output.
        </kb:description>
      </kb:purpose>
      <kb:purpose id="deliverSimpleJavaApplicationsOutput" root="true">
        <kb:label>deliver: Output of Simply Operating Java Applications</kb:label>
        <kb:description>
        [something to] deliver the output of Java applications to a requesting 
           client whereas these Java applications shall not be able to incorporate
           persistently stored database data for dynamically computing the output.
        </kb:description>
      </kb:purpose>
      <kb:purpose id="deliverPages" root="false">
        <kb:label>deliver: Pages</kb:label>
      </kb:purpose>
      <kb:purpose id="execScripts" root="false">
        <kb:label>execute: Scripts</kb:label>
      </kb:purpose>
      <kb:purpose id="linkToDataStore" root="false">
        <kb:label>link to: Data store </kb:label>
      </kb:purpose>
      <kb:purpose id="storeData" root="false">
        <kb:label>store : Data</kb:label>
      </kb:purpose>
      <kb:purpose id="execEeBeans" root="false">
        <kb:label>execute : EE Beans</kb:label>
      </kb:purpose>
      <kb:purpose id="execJavaApps" root="false">
        <kb:label>execute : Java Applications</kb:label>
      </kb:purpose>
    </kb:purposes>
    
    <kb:variants>
      <kb:variant id="WeSeScriSoSt">
        <kb:label>Apache-Scripting-Software-Stack</kb:label>
        <kb:description>
        An Apache based web server software stack enriched by particular scripting 
           modules for delivering dynamically composed web pages.
        </kb:description>
      </kb:variant>
      
      <kb:variant id="WeSeDbScriSoSt">
        <kb:label>Apache-Database-Scripting-Software-Stack</kb:label>
        <kb:description>
        An Apache based web server software stack being able to use database data
           and being enriched by particular scripting modules for delivering 
           dynamically composed web pages
        </kb:description>
      </kb:variant>
      
      <kb:variant id="JaEjbDbSoSt">
        <kb:label>Java-Ejb-Database-Software-Stack</kb:label>
        <kb:description>
        A java application server software stack being able to use database data
           and to operate on java beans for offering web services etc.
        </kb:description>
      </kb:variant>
      
      <kb:variant id="JaWapDbSoSt">
        <kb:label>Java-Web-App-Database-Software-Stack</kb:label>
        <kb:description>
        A java application server software stacke being able to compute its output 
           on the base of database data.
        </kb:description>
      </kb:variant>
      
      <kb:variant id="Apache">
        <kb:label>Apache</kb:label>
        <kb:description>
        The famous Apache Webserver
        </kb:description>
      </kb:variant>
      
      <kb:variant id="ModPhp">
        <kb:label>Apache-PHP-Module</kb:label>
      </kb:variant>
      
      <kb:variant id="ModPython">
        <kb:label>Apache-Python-Module</kb:label>
      </kb:variant>
      
      <kb:variant id="MysqlPhpConnector">
        <kb:label>Mysql-PHP-Connector</kb:label>
      </kb:variant>
      
      <kb:variant id="MysqlPythonConnector">
        <kb:label>Mysql-Python-Connector</kb:label>
      </kb:variant>
      
      <kb:variant id="MysqlJavaConnector">
        <kb:label>Mysql-JDBC-Connector</kb:label>
      </kb:variant>
      
      <kb:variant id="PostgresPhpConnector">
        <kb:label>Postgres-PHP-Connector</kb:label>
      </kb:variant>
      
      <kb:variant id="PostgresPythonConnector">
        <kb:label>Postgres-Python-Connector</kb:label>
      </kb:variant>
      
      <kb:variant id="PostgresJavaConnector">
        <kb:label>Postgres-JDBC-Connector</kb:label>
      </kb:variant>
      
      <kb:variant id="Postgres">
        <kb:label>Postgres</kb:label>
      </kb:variant>
      
      <kb:variant id="Mysql">
        <kb:label>Mysql</kb:label>
      </kb:variant>
      
      <kb:variant id="Jboss">
        <kb:label>JBossr</kb:label>
      </kb:variant>
      
      <kb:variant id="Tomcat">
        <kb:label>Tomcat</kb:label>
      </kb:variant>
      
    </kb:variants>  
    
    <kb:alternatives>
      <kb:isFulfilledBy purposeRef="deliverStaticWebPages" 
        variantRefs="Apache " />
      <kb:isFulfilledBy purposeRef="deliverDynamicWebPages" 
        variantRefs="WeSeScriSoSt WeSeDbScriSoSt " />
      <kb:isFulfilledBy purposeRef="deliverPersistentDynamicWebPages" 
        variantRefs="JaEjbDbSoSt JaWapDbSoSt " />
      <kb:isFulfilledBy purposeRef="deliverSimpleJavaApplicationsOutput" 
        variantRefs="Tomcat " />
      <kb:isFulfilledBy purposeRef="deliverPages" 
        variantRefs="Apache " />
      <kb:isFulfilledBy purposeRef="execScripts" 
        variantRefs="ModPhp ModPython " />
      <kb:isFulfilledBy purposeRef="linkToDataStore" 
        variantRefs="MysqlPhpConnector MysqlPythonConnector MysqlJavaConnector PostgresPhpConnector PostgresPythonConnector PostgresJavaConnector " />
      <kb:isFulfilledBy purposeRef="storeData" 
        variantRefs="Mysql Postgres " />
      <kb:isFulfilledBy purposeRef="execEeBeans" 
        variantRefs="Jboss " />
      <kb:isFulfilledBy purposeRef="execJavaApps" 
        variantRefs="Tomcat " />
    </kb:alternatives>  
     
    <kb:constituents>
      <kb:isConstitutedBy variantRef="WeSeScriSoSt">
        <kb:constituent many="1" purposeRef="deliverPages" />
        <kb:constituent many="1" purposeRef="execScripts" />
      </kb:isConstitutedBy>  
      <kb:isConstitutedBy variantRef="WeSeDbScriSoSt">
        <kb:constituent many="1" purposeRef="deliverPages" />
        <kb:constituent many="1" purposeRef="execScripts" />
        <kb:constituent many="1" purposeRef="linkToDataStore" />
        <kb:constituent many="1" purposeRef="storeData" />
      </kb:isConstitutedBy>  
      <kb:isConstitutedBy variantRef="JaEjbDbSoSt">
        <kb:constituent many="1" purposeRef="linkToDataStore" />
        <kb:constituent many="1" purposeRef="storeData" />
        <kb:constituent many="1" purposeRef="execEeBeans" />
      </kb:isConstitutedBy>  
      <kb:isConstitutedBy variantRef="JaWapDbSoSt">
        <kb:constituent many="1" purposeRef="linkToDataStore" />
        <kb:constituent many="1" purposeRef="storeData" />
        <kb:constituent many="1" purposeRef="execJavaApps" />
      </kb:isConstitutedBy>  
    </kb:constituents> 
    
    <kb:events>
      <kb:event id="WeSeDbScriSoStAsTrigger" 
        nexusId="WeSeDbScriSoSt" 
        contextPsPvRefs="WeSeDbScriSoSt">
        <kb:label>The Context of the Web Server DB Scripting Software Stack</kb:label>
        <kb:description>
        TRUE whenever you are deriving this stack
        </kb:description>
        <kb:variantTrigger variantRef="WeSeDbScriSoSt" />
      </kb:event>
      <kb:event id="WeSeDbWithMysqlPhpConnex" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt linkToDataStore ">
        <kb:label>DB based web server with mysql-php-connector</kb:label>
        <kb:variantTrigger variantRef="MysqlPhpConnector" />
      </kb:event>
      <kb:event id="WeSeDbWithMysqlPythonConnex" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt linkToDataStore ">
        <kb:label>DB based web server with mysql-phython-connector</kb:label>
        <kb:variantTrigger variantRef="MysqlPythonConnector" />
      </kb:event>
      <kb:event id="WeSeDbWithPostgresPhpConnex" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt linkToDataStore ">
        <kb:label>DB based web server with postgres-php-connector</kb:label>
        <kb:variantTrigger variantRef="PostgresPhpConnector" />
      </kb:event>
      <kb:event id="WeSeDbWithPostgresPythonConnex" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt linkToDataStore ">
        <kb:label>DB based web server with postgres-phython-connector</kb:label>
        <kb:variantTrigger variantRef="PostgresPythonConnector" />
      </kb:event>
      <kb:event id="WeSeDbWithModPython" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt execScripts ">
        <kb:label>DB based web server with mod-python</kb:label>
        <kb:variantTrigger variantRef="ModPython" />
      </kb:event>
      <kb:event id="WeSeDbWithModPhp" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt execScripts ">
        <kb:label>DB based web server with mod-php</kb:label>
        <kb:variantTrigger variantRef="ModPhp" />
      </kb:event>
      <kb:event id="WeSeDbWithMysql" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt storeData ">
        <kb:label>DB based web server with mysql</kb:label>
        <kb:variantTrigger variantRef="Mysql" />
      </kb:event>
      <kb:event id="WeSeDbWithPostgres" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt storeData ">
        <kb:label>DB based web server with postgres</kb:label>
        <kb:variantTrigger variantRef="Postgres" />
      </kb:event>
      <kb:event id="JaEjbDbWithMysqlJavaConnector" 
        nexusId="JaEjbDbSoSt"
        contextPsPvRefs="JaEjbDbSoSt linkToDataStore ">
        <kb:label>Java Ejb Database Software Stack mit Mysql Java Connector</kb:label>
        <kb:variantTrigger variantRef="MysqlJavaConnector" />
      </kb:event>
      <kb:event id="JaEjbDbWithPostgresJavaConnector" 
        nexusId="JaEjbDbSoSt"
        contextPsPvRefs="JaEjbDbSoSt linkToDataStore ">
        <kb:label>Java Ejb Database Software Stack mit Postgres Java Connector</kb:label>
        <kb:variantTrigger variantRef="PostgresJavaConnector" />
      </kb:event>
      <kb:event id="JaEjbDbWithMysql" 
        nexusId="JaEjbDbSoSt"
        contextPsPvRefs="JaEjbDbSoSt storeData ">
        <kb:label>Java Ejb Database Software Stack mit Mysql DBMS</kb:label>
        <kb:variantTrigger variantRef="Mysql" />
      </kb:event>
      <kb:event id="JaEjbDbWithPostgres" 
        nexusId="JaEjbDbSoSt"
        contextPsPvRefs="JaEjbDbSoSt storeData ">
        <kb:label>Java Ejb Database Software Stack mit Postgres DBMS</kb:label>
        <kb:variantTrigger variantRef="Postgres" />
      </kb:event>
      <kb:event id="JaWapDbWithMysqlJavaConnector" 
        nexusId="JaWapDbSoSt"
        contextPsPvRefs="JaWapDbSoSt linkToDataStore ">
        <kb:label>Java Web App Database Software Stack mit Mysql Java Connector</kb:label>
        <kb:variantTrigger variantRef="MysqlJavaConnector" />
      </kb:event>
      <kb:event id="JaWapDbWithPostgresJavaConnector" 
        nexusId="JaWapDbSoSt"
        contextPsPvRefs="JaWapDbSoSt linkToDataStore ">
        <kb:label>Java Web App Database Software Stack mit Postgres Java Connector</kb:label>
        <kb:variantTrigger variantRef="PostgresJavaConnector" />
      </kb:event>
      <kb:event id="JaWapDbWithMysql" 
        nexusId="JaWapDbSoSt"
        contextPsPvRefs="JaWapDbSoSt storeData ">
        <kb:label>Java Web App Database Software Stack mit Mysql </kb:label>
        <kb:variantTrigger variantRef="Mysql" />
      </kb:event>
      <kb:event id="JaWapDbWithPostgres" 
        nexusId="JaWapDbSoSt"
        contextPsPvRefs="JaWapDbSoSt storeData ">
        <kb:label>Java Web App Database Software Stack mit Postgres </kb:label>
        <kb:variantTrigger variantRef="Postgres" />
      </kb:event>
      <kb:event id="WeSeDbScrSoStWithoutMysqlJavaConnex" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt linkToDataStore ">
        <kb:label>TRUE IF MysqlJavaConnector not used as Db connector 
           in the context of aggregating a WeSeDbScriSoSt</kb:label>
        <kb:not />
        <kb:variantTrigger variantRef="MysqlJavaConnector" />
      </kb:event>
      <kb:event id="WeSeDbScrSoStWithoutPostgresJavaConnex" 
        nexusId="WeSeDbScriSoSt"
        contextPsPvRefs="WeSeDbScriSoSt linkToDataStore ">
        <kb:label>TRUE IF PostgresJavaConnector not used as Db connector 
           in the context of aggregating a WeSeDbScriSoSt</kb:label>
        <kb:not />
        <kb:variantTrigger variantRef="PostgresJavaConnector" />
      </kb:event>
    </kb:events> 
    
    
   <kb:rules>
     <kb:rule id="WeSeScriSoStWithoutMysqlJavaConnex"
        premiseEventRefs="WeSeDbScriSoStAsTrigger" 
        conclusioEventRef="WeSeDbScrSoStWithoutMysqlJavaConnex" > 
       <kb:label>Exclud the use of Mysql Java Connector in the context 
       of aggregating a WebServer Scripting with DB Software Stack</kb:label>
     </kb:rule>
     <kb:rule id="WeSeScriSoStWithoutPostgresJavaConnex"
        premiseEventRefs="WeSeDbScriSoStAsTrigger" 
        conclusioEventRef="WeSeDbScrSoStWithoutPostgresJavaConnex" > 
       <kb:label>Exclud the use of Postgres Java Connector in the context 
       of aggregating a WebServer Scripting with DB Software Stack</kb:label>
     </kb:rule>

     <kb:rule id="IFmysqlPhpConnexTHENModPhp"
       premiseEventRefs="WeSeDbWithMysqlPhpConnex »" 
       conclusioEventRef="WeSeDbWithModPhp" > 
       <kb:label>IF mysql-php-connector THEN mod-php</kb:label>
       <kb:description>
       R-1.1
       </kb:description>
     </kb:rule>
     <kb:rule id="IFmysqlPythonConnexTHENModPython"
       premiseEventRefs="WeSeDbWithMysqlPythonConnex »" 
       conclusioEventRef="WeSeDbWithModPython" > 
       <kb:label>IF mysql-python-connector THEN mod-python</kb:label>
       <kb:description>
       R-1.2
       </kb:description>
     </kb:rule>
     <kb:rule id="IFpostgresPhpConnexTHENModPhp"
       premiseEventRefs="WeSeDbWithPostgresPhpConnex »" 
       conclusioEventRef="WeSeDbWithModPhp" > 
       <kb:label>IF postgres-php-connector THEN mod-php</kb:label>
       <kb:description>
       R-1.3
       </kb:description>
     </kb:rule>
     <kb:rule id="IFpostgresPythonConnexTHENModPython"
       premiseEventRefs="WeSeDbWithPostgresPythonConnex »" 
       conclusioEventRef="WeSeDbWithModPython" > 
       <kb:label>IF postgres-python-connector THEN mod-python</kb:label>
       <kb:description>
       R-1.4
       </kb:description>
     </kb:rule>
     <kb:rule id="IFmysqlPhpConnexTHENmysqlDb"
       premiseEventRefs="WeSeDbWithMysqlPhpConnex »" 
       conclusioEventRef="WeSeDbWithMysql" > 
       <kb:label>IF mysql-php-connector THEN mysql dbms</kb:label>
       <kb:description>
       R-2.1
       </kb:description>
     </kb:rule>
     <kb:rule id="IFmysqlPythonConnexTHENmysqlDb"
       premiseEventRefs="WeSeDbWithMysqlPythonConnex »" 
       conclusioEventRef="WeSeDbWithMysql" > 
       <kb:label>IF mysql-python-connector THEN mysql dbms</kb:label>
       <kb:description>
       R-2.2
       </kb:description>
     </kb:rule>
     <kb:rule id="IFpostgresPhpConnexTHENmysqlDb"
       premiseEventRefs="WeSeDbWithPostgresPhpConnex »" 
       conclusioEventRef="WeSeDbWithPostgres" > 
       <kb:label>IF postgres-php-connector THEN postgres dbms</kb:label>
       <kb:description>
       R-2.3
       </kb:description>
     </kb:rule>
     <kb:rule id="IFpostgresPythonConnexTHENmysqlDb"
       premiseEventRefs="WeSeDbWithPostgresPythonConnex »" 
       conclusioEventRef="WeSeDbWithPostgres" > 
       <kb:label>IF postgres-python-connector THEN postgres dbms</kb:label>
       <kb:description>
       R-2.4
       </kb:description>
     </kb:rule>
     <kb:rule id="IFJaEjbDbWithMysqlJavaConnectorTHENJaEjbDbWithMysql"
       premiseEventRefs="JaEjbDbWithMysqlJavaConnector »" 
       conclusioEventRef="JaEjbDbWithMysql" > 
       <kb:label>Mysql Connector in context of JavaEjbDbSoST requires Mysql DBMS</kb:label>
       <kb:description>
       R-3.1
       </kb:description>
     </kb:rule>
     <kb:rule id="IFJaEjbDbWithPostgresJavaConnectorTHENJaEjbDbWithPostgres"
       premiseEventRefs="JaEjbDbWithPostgresJavaConnector »" 
       conclusioEventRef="JaEjbDbWithPostgres" > 
       <kb:label>Postgress Connector in context of JavaEjbDbSoST requires Postgres DBMS</kb:label>
       <kb:description>
       R-3.2
       </kb:description>
     </kb:rule>
     <kb:rule id="IFJaWapDbWithPostgresJavaConnectorTHENJaWapDbWithPostgres"
       premiseEventRefs="JaWapDbWithMysqlJavaConnector »" 
       conclusioEventRef="JaWapDbWithMysql" > 
       <kb:label>Mysql Connector in context of JavaWapDbSoST requires Mysql DBMS</kb:label>
       <kb:description>
       R-3.3
       </kb:description>
     </kb:rule>
     <kb:rule id="IFJaWapDbWithPostgresJavaConnectorTHENJaWapDbWithPostgres"
       premiseEventRefs="JaWapDbWithPostgresJavaConnector »" 
       conclusioEventRef="JaWapDbWithPostgres" > 
       <kb:label>Postgress Connector in context of JavaWapDbSoST requires Postgres DBMS</kb:label>
       <kb:description>
       R-3.4
       </kb:description>
     </kb:rule>
   </kb:rules> 
  </kb:data>    
